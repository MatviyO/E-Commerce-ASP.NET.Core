

<ul id="sideManu" class="nav nav-tabs nav-stacked">
  @foreach (var category in Model)
  {
    <li class="subMenu open">
      <a> @category.Name</a>
      @if (category.InverseParents != null && category.InverseParents.Count > 0)
      {
        <ul style="display: none;">
          @foreach (var subCateogory in category.InverseParents)
          {

            @if (subCateogory.Status)
            {
              <li><a class="active" href="products.html"><i class="icon-chevron-right">@subCateogory.Name</i> </a></li>

            }
          }
        </ul>
      }
    </li>
  }
</ul>